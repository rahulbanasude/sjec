<div class="subjects">
<div class="w3-container">
  <hr>
  <div class="w3-center">
    <h2>Listing Subjects</h2>
  </div>
<div class="w3-responsive w3-card-4">
<table class="w3-table w3-striped w3-bordered">
<thead>
<tr class="w3-theme">
  <th>Subjects</th>
  <th>Links</th>

</tr>
</thead>
<tbody>
<% @papers.each do |paper| %>
<tr>
  <td><%= paper.name %></td>
  <td><%= link_to "Download", paper.paper.url, target: '_blank'%></td>
  <% end %>
</tr>
</tbody>
</table>
</div>
 <a class="w3-btn w3-theme"<%= link_to 'Back', :back%></a>
</div>
</div>
